{
  "templates":
    [
      {
        "match": { "config": "mpl", "exp": "time" },
        "template": "/usr/bin/time -v bin/<bench>.<config>.bin @mpl <procs?procs {}> set-affinity -- <args?{} ><args1?{}> -repeat 20 -warmup 5"
      },
      {
        "match": { "config": "mpl", "exp": "single-space" },
        "template": "/usr/bin/time -v bin/<bench>.<config>.bin @mpl <procs?procs {}> set-affinity -- <args?{} ><args1?{}> -repeat 1 -warmup 0"
      }
    ],

  "globals":
    {
      "config": ["mpl-em"],
      "cwd": ["mpl"],
      "exp": ["time","single-space"],
      "trials": ["1","10"]
    },

  "jqselect":
    "(  (.exp == \"time\" and .trials == \"1\")  or  ( .exp == \"single-space\" and .trials == \"10\")  )",

  "specs":
    [
      {
        "tag": ["tangle"],
        "bench": ["tangle"],
        "args": ["-elem-size 1000000 -num-elems 200 -num-tangles 10"],
        "args1": [
          "-p-ent 0.0", "-p-ent 0.1", "-p-ent 0.2", "-p-ent 0.3", "-p-ent 0.4",
          "-p-ent 0.5", "-p-ent 0.6", "-p-ent 0.7", "-p-ent 0.8", "-p-ent 0.9",
          "-p-ent 1.0"
        ]
      },
      {
        "tag": ["tangle-small"],
        "bench": ["tangle"],
        "args": ["-elem-size 500000 -num-elems 200 -num-tangles 10"],
        "args1": [
          "-p-ent 0.0", "-p-ent 0.1", "-p-ent 0.2", "-p-ent 0.3", "-p-ent 0.4",
          "-p-ent 0.5", "-p-ent 0.6", "-p-ent 0.7", "-p-ent 0.8", "-p-ent 0.9",
          "-p-ent 1.0"
        ]
      },
      {
        "tag": ["tangle-small2"],
        "bench": ["tangle"],
        "args": ["-elem-size 100000 -num-elems 1000 -num-tangles 10"],
        "args1": [
          "-p-ent 0.0", "-p-ent 0.01", "-p-ent 0.02", "-p-ent 0.03", "-p-ent 0.04",
          "-p-ent 0.05", "-p-ent 0.06", "-p-ent 0.07", "-p-ent 0.08", "-p-ent 0.09",
          "-p-ent 0.1"
        ]
      }
    ]
}
