{
  "templates":
    [
      {
        "match": { "exp": "time", "config": "go", "procs": "(?!1$)" },
        "template": "/usr/bin/time -v numactl -i all -- bin/<bench>.<config>.bin -procs <procs> <args?{}> <args1?{}> -repeat 20 -warmup 5"
      },
      {
        "match": { "exp": "time", "config": "go" },
        "template": "/usr/bin/time -v bin/<bench>.<config>.bin -procs <procs> <args?{}> <args1?{}> -repeat 20 -warmup 5"
      },
      {
        "match": { "exp": "space", "config": "go", "procs": "(?!1$)" },
        "template": "/usr/bin/time -v numactl -i all -- bin/<bench>.<config>.bin -procs <procs> <args?{}> <args1?{}> -repeat 20 -warmup 0"
      },
      {
        "match": { "exp": "space", "config": "go" },
        "template": "/usr/bin/time -v bin/<bench>.<config>.bin -procs <procs> <args?{}> <args1?{}> -repeat 20 -warmup 0"
      }
    ],

  "globals":
    {
      "config": ["go"],
      "cwd": ["go"],
      "exp": ["time","space"],
      "trials": ["1"]
    },

  "jqselect": "(true)",

  "specs":
    [
      {
        "tag": ["forgo-sort-int64"],
        "bench": ["forgo-sort"],
        "args": ["-n 20000000"]
      },
      {
        "tag": ["matthew-sort-int64"],
        "bench": ["matthew-sort"],
        "args": ["-n 20000000"]
      },
      {
        "tag": ["primes"],
        "bench": ["primes"],
        "args": ["-n 100000000"]
      }
    ]
}
