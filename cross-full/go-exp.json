{
  "templates":
    [
      {
        "match": { "exp": "time", "config": "go", "procs": "(?!1$)" },
        "template": "/usr/bin/time -v bin/<bench>.<config>.bin -procs <procs> <args?{}> <args1?{}> -repeat 20 -warmup 5"
      },
      {
        "match": { "exp": "time", "config": "go" },
        "template": "/usr/bin/time -v bin/<bench>.<config>.bin -procs <procs> <args?{}> <args1?{}> -repeat 20 -warmup 5"
      },
      {
        "match": { "exp": "space", "config": "go", "procs": "(?!1$)" },
        "template": "/usr/bin/time -v bin/<bench>.<config>.bin -procs <procs> <args?{}> <args1?{}> -repeat 20 -warmup 0"
      },
      {
        "match": { "exp": "space", "config": "go" },
        "template": "/usr/bin/time -v bin/<bench>.<config>.bin -procs <procs> <args?{}> <args1?{}> -repeat 20 -warmup 0"
      }
    ],

  "globals":
    {
      "config": ["go"],
      "cwd": ["go"],
      "exp": ["time","space"],
      "trials": ["1"]
    },

  "jqselect":
    "(  (.exp == \"time\" and .trials == \"1\" and (.procs == \"1\" or .procs == $max_procs))  or  ( .exp == \"space\" and ((.procs == \"1\" and .trials == \"1\") or (.procs == $max_procs)) )  )",

  "specs":
    [
      {
        "tag": ["msort"],
        "bench": ["msort"],
        "args": ["-n 20000000"]
      },
      {
        "tag": ["dedup-strings"],
        "bench": ["dedup-strings"],
        "args": ["-infile ../inputs/words256-shuffled.txt"]
      },
      {
        "tag": ["primes"],
        "bench": ["primes"],
        "args": ["-n 100000000"]
      },
      {
        "tag": ["tokens"],
        "bench": ["tokens"],
        "args": ["-infile ../inputs/words256.txt"]
      },
      {
        "tag": ["mcss"],
        "bench": ["mcss"],
        "args": ["-n 500000000"]
      },
      {
        "tag": ["linefit-accum"],
        "bench": ["linefit-accum"],
        "args": ["-n 500000000"]
      },
      {
        "tag": ["sparse-mxv"],
        "bench": ["sparse-mxv"],
        "args": ["-n 200000000"]
      },
      {
        "tag": ["wc-accum"],
        "bench": ["wc-accum"],
        "args": ["-infile ../inputs/rmat-10M-symm"]
      }
    ]
}
