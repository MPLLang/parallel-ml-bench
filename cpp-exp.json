{
  "templates":
    [
      {
        "match": { "config": "cpp", "procs": "(?!1$)" },
        "template": "<procs?PARLAY_NUM_THREADS={}> /usr/bin/time -v numactl -i all -- bin/<bench>.<config>.bin <args?{}> <args1?{}> -repeat 10 -warmup 5"
      },
      {
        "match": { "config": "cpp" },
        "template": "<procs?PARLAY_NUM_THREADS={}> /usr/bin/time -v bin/<bench>.<config>.bin <args?{}> <args1?{}> -repeat 10 -warmup 5"
      }
    ],

  "globals":
    {
      "config": ["cpp"],
      "cwd": ["cpp"],
      "exp": ["time"],
      "trials": ["1"]
    },

  "jqselect": "(true)",

  "specs":
    [
      {
        "tag": ["primes"],
        "bench": ["primes"],
        "args": ["-n 100000000"]
      },
      {
        "tag": ["delaunay"],
        "bench": ["delaunay"],
        "args": ["-input ../inputs/uniform-circle-1M"]
      }
    ]
}
