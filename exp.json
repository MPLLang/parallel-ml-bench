      },
      {
        "tag": ["ms-queue"],
        "bench": ["ms-queue"],
        "args": ["-nq 100 -bs 1000000 -work 1000"]
      },
      {
        "tag": ["harris-linked-list"],
        "bench": ["harris-linked-list"],
        "args": ["-nq 100 -bs 100000 -work 17"]
      },
      {
        "tag": ["linden-pq"],
        "bench": ["linden-pq"],
        "args": ["-nq 100 -bs 100000 -work 17"]
      },
      {
        "tag": ["pfa-bst"],
        "bench": ["pfa-bst"],
        "args": ["-n 10000 -nq 10000000 -nu 50"]
